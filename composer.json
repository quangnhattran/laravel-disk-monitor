{
    "name": "quangvision/laravel-disk-monitor",
    "description": "Monitor metrics of Laravel disks",
    "keywords": [
        "quangvision",
        "laravel",
        "laravel-disk-monitor"
    ],
    "homepage": "https://github.com/quangvision/laravel-disk-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Quang Tran",
            "email": "quangvision@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Qt\\DiskMonitor\\": "src",
            "Qt\\DiskMonitor\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qt\\DiskMonitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qt\\DiskMonitor\\DiskMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
